@{
    ViewData["Title"] = "Races on map";
    var races = (List<Race>)ViewData["races"];

}

<img src="~/images/allerlei/map.png" width="1024" height="468" alt="racemap" usemap="#racemap"/>

<map name="racemap" >
    @foreach (var race in races) {
        <area shape="circle" href="/Home/ShowRace/@race.RaceID" coords="@race.X, @race.Y, 8" alt="race.Name">
    }
</map>