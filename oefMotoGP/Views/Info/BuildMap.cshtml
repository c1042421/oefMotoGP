@{
    ViewData["Title"] = "Races on map";
    ViewData["pageTitle"] = "Races on map";
    ViewData["bannerNaam"] = "bannerRaces.jpg";
}

<img src="~/images/allerlei/map.png" width="1024" height="468" alt="racemap" usemap="#racemap"/>

<map name="racemap" >
    @foreach (var race in Model) {
        <area shape="circle" href="/Info/ShowRace/@race.RaceID" coords="@race.X, @race.Y, 8" alt="race.Name">
    }
</map>